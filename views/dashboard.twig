{% extends 'base.twig' %}

{% block body %}
    <div align="center">

        <h3>
            Welcome {{UserName}} &nbsp;!
        </h3>
        <div align="center">
            {% for message in app.session.getFlashBag.get('alert_success') %}
                <div  class="alert alert-success">
                    <p>{{message}}</p>
                </div>
            {%endfor%}
        </div>

        {% if examset is defined and examset == 'true' %}<img src="{{ constant('BASEPATH') }}/assets/new.gif"> &nbsp;&nbsp; <a href = '{{ constant('BASEPATH') }}/examstart/{{UserId}}'>Start examination</a>{% endif %}

            {% if success is defined and success == 1%} <img src="{{ constant('BASEPATH') }}/assets/new.gif"> <br> You are qualified for interview! <br> Please contact office... {% endif %}

        </div>
        {% endblock %}
